{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "process_column": {"type": ["string", "null"]},
    "server_column": {"type": ["string", "null"]},
    "queue_column": {"type": ["string", "null"]},
    "eligible_column": {"type": ["string", "null"]},
    "start_column": {"type": ["string", "null"]},
    "end_column": {"type": ["string", "null"]},
    "duration_column": {"type": ["string", "null"]},
    "close_cycle_start_day": {"type": "integer", "minimum": 1, "maximum": 31},
    "close_cycle_end_day": {"type": "integer", "minimum": 1, "maximum": 31},
    "roll_day_fallback": {"type": "integer", "minimum": 1, "maximum": 15},
    "min_process_runs": {"type": "integer", "minimum": 1},
    "min_assignment_servers": {"type": "integer", "minimum": 1},
    "min_modeled_delta_hours": {"type": "number", "minimum": 0.0},
    "assignment_benefit_factor": {"type": "number", "minimum": 0.0},
    "assignment_penalty_scale": {"type": "number", "minimum": 0.0},

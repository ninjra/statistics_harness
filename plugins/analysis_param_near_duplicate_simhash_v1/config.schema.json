        "string",
        "null"
      ]
    },
    "max_entities_per_process": {
      "default": 3000,
      "type": [
        "integer",
        "null"
      ]
    },
    "max_hamming_distance": {
      "default": 3,
      "type": [
        "integer",
        "null"
      ]
    },
    "max_processes": {
      "default": 60,
      "type": [
        "integer",
        "null"
      ]
    },
    "min_cluster_size": {
      "default": 5,
      "type": [
        "integer",
        "null"
      ]
